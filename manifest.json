{
    "name": "Counter",
    "description": "Adds and subtracts at the press of a button",
    "manifest_version": 2,
    "version": "1",
    "icons": { "16": "icon.png" },

  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
   },

   "permissions": [
          "storage"
        ],

  "content_scripts": [{
    "matches": ["http://www.google.com/*"],
    "js": ["script.js"]
  }],
  "manifest_version": 2
}
