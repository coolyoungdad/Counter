{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["jquery-3.1.1.slim.min.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
        "js": [ "jquery-3.1.1.slim.min.js","background.js" ],
        "run_at": "document_end"
    } 
  ],
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
